<Window x:Class="VisualGGPK2.VersionSelector"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="GameVersion"
		Width="170"
		Height="230"
		ResizeMode="NoResize"
		WindowStyle="ToolWindow"
		WindowStartupLocation="CenterScreen">
	<StackPanel>
		<Button Content="International&#xa;      Steam"
				FontSize="16"
				Width="120"
				Height="50"
				Margin="0, 20, 0, 0"
				Click="International_Click"/>
		<Button Content="Garena"
				FontSize="16"
				Width="120"
				Height="30"
				Margin="0, 20, 0, 0"
				Click="Garena_Click"/>
		<Button Content="Tencent"
				FontSize="16"
				Width="120"
				Height="30"
				Margin="0, 20, 0, 0"
				Click="Tencent_Click"/>
	</StackPanel>
</Window>